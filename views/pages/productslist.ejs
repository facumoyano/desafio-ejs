<!DOCTYPE html>
<html lang="en">
    <head>
        <%- include('../partials/head'); %>
    </head>
    <body>
        <%- include('../partials/navbar'); %>
        <h1 class="title-products">Lista de productos</h1>
        <table class="table">
            <thead>
                <tr>
                    <th>Nombre</th>
                    <th>Precio</th>
                    <th>Foto</th>
                </tr>
            </thead>
        <% products.forEach((item)=>{ %>
            <tbody>
                <tr >
                    <td><%=item.nombre%> </td>
                    <td>$<%=item.precio%></td>
                    <td>
                        <img src="<%=item.imgUrl %>" />
                    </td>
                </tr>
            </tbody>
        <% }) %>
    </body>
</html>
